{
    "description": "Flow Framework package to check the health status of you application",
    "type": "neos-package",
    "name": "t3n/flow-healthstatus",
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "require": {
        "neos/flow": "~6.0"
    },
    "require-dev": {
        "t3n/coding-standard": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "t3n\\Flow\\HealthStatus\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "t3n.Flow.HealthStatus"
        }
    }
}
